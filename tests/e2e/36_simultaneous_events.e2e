# Test multiple simultaneous events
reset_game
set_spawn_enabled 0
wait_frames 5

# Trigger two events at once
trigger_event rain 10
trigger_event heatwave 10
wait_frames 5

# Both should be active
assert_event_active rain
assert_event_active heatwave

# Wait for events to expire
wait 12

# Both should be gone
assert_event_inactive rain
assert_event_inactive heatwave
